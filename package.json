{
	"name": "@rasmuslp/eslint-config",
	"version": "8.4.0",
	"description": "My configs",
	"homepage": "https://github.com/rasmuslp/eslint-config-rasmuslp#readme",
	"bugs": {
		"url": "https://github.com/rasmuslp/eslint-config-rasmuslp/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rasmuslp/eslint-config-rasmuslp.git"
	},
	"license": "BSD-3-Clause",
	"author": {
		"name": "Rasmus Ljungmann Pedersen",
		"email": "rasmuslp@gmail.com"
	},
	"type": "module",
	"main": "src/index.js",
	"files": [
		"src/"
	],
	"scripts": {
		"config-inspector": "npx @eslint/config-inspector@latest",
		"format": "npm run prettier:write",
		"lint": "eslint .",
		"prettier:check": "prettier . --check",
		"prettier:write": "prettier . --write",
		"publish:beta": "np --no-publish --no-release-draft --any-branch",
		"publish:release": "np --no-publish --no-release-draft",
		"test": "npm run format && npm run lint"
	},
	"dependencies": {
		"@eslint/js": "^9.39.0",
		"eslint-config-flat-gitignore": "^2.1.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-unicorn": "^62.0.0",
		"typescript-eslint": "^8.46.2"
	},
	"devDependencies": {
		"eslint": "9.39.0",
		"eslint-plugin-jest": "29.0.1",
		"eslint-plugin-n": "17.23.1",
		"np": "10.2.0",
		"prettier": "3.6.2",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"eslint": ">=9.39.0",
		"eslint-plugin-jest": ">=29.0.1",
		"eslint-plugin-n": ">=17.23.1",
		"typescript": ">=5.9.3"
	},
	"peerDependenciesMeta": {
		"eslint-plugin-jest": {
			"optional": true
		},
		"eslint-plugin-n": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=22.11.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
